         <DisplayName>DFNC Minor Rev</DisplayName>
         <Visibility>Invisible</Visibility>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <Value>0</Value>
      </Integer>
      <Integer Name="DeviceSFNCVersionMajor" NameSpace="Standard">
         <ToolTip>SFNC Major revision </ToolTip>
         <Description>Major Version of the Genicam Standard Features Naming Convention which was used to create the device's XML.</Description>
         <DisplayName>SFNC Major Rev</DisplayName>
         <Visibility>Invisible</Visibility>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <Value>1</Value>
      </Integer>
      <Integer Name="DeviceSFNCVersionMinor" NameSpace="Standard">
         <ToolTip>SFNC Minor revision </ToolTip>
         <Description>Minor Version of the Genicam Standard Features Naming Convention which was used to create the device's XML.</Description>
         <DisplayName>SFNC Minor Rev</DisplayName>
         <Visibility>Invisible</Visibility>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <Value>5</Value>
      </Integer>
      <Integer Name="DeviceSFNCVersionSubMinor" NameSpace="Standard">
         <ToolTip>SFNC SubMinor revision </ToolTip>
         <Description>SubMinor Version of the Genicam Standard Features Naming Convention which was used to create the device's XML.</Description>
         <DisplayName>SFNC SubMinor Rev</DisplayName>
         <Visibility>Invisible</Visibility>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <Value>1</Value>
      </Integer>
      <Command Name="deviceBIST" NameSpace="Custom">
         <ToolTip>Device Built-In Self Test</ToolTip>
         <Description>Command to perform an internal test which will determine the device status.</Description>
         <DisplayName>Device Built-In Self Test</DisplayName>
         <Visibility>Beginner</Visibility>
         <pValue>pdeviceBISTCmdReg</pValue>
         <CommandValue>1</CommandValue>
      </Command>
      <IntReg Name="pdeviceBISTCmdReg">
         <Address>0x18008190</Address>
         <Length>4</Length>
         <AccessMode>WO</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Enumeration Name="deviceBISTStatus" NameSpace="Custom">
         <ToolTip>Device Built-In Self Test Status</ToolTip>
         <Description>Return the status of the device Built-In Self test. Possible return values are device-specific.</Description>
         <DisplayName>Device Built-In Self Test Status</DisplayName>
         <Visibility>Beginner</Visibility>
         <EnumEntry Name="Passed">
            <ToolTip>No failure detected</ToolTip>
            <Description>No failure detected</Description>
            <DisplayName>Passed</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="FirmwareUpdateFailure">
            <ToolTip>Last firmware update operation failed</ToolTip>
            <Description>Last firmware update operation failed</Description>
            <DisplayName>Last firmware update failed</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>1</Value>
         </EnumEntry>
         <EnumEntry Name="FPGA_CRC_Failure">
            <ToolTip>FPGA Cyclic Redundancy Check Failed</ToolTip>
            <Description>FPGA Cyclic Redundancy Check Failed</Description>
            <DisplayName>FPGA Cyclic Redundancy Check Failed</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>2</Value>
         </EnumEntry>
         <EnumEntry Name="Unexpected_Error">
            <ToolTip>Unexpected Error</ToolTip>
            <Description>Switched to recovery mode due to unexpected software error.</Description>
            <DisplayName>Unexpected Error</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>4</Value>
         </EnumEntry>
         <pValue>pdeviceBISTStatusReg</pValue>
      </Enumeration>
      <IntReg Name="pdeviceBISTStatusReg">
         <Address>0x180081A0</Address>
         <Length>4</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pdeviceBISTCmdReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Integer Name="deviceBISTStatusAll">
         <ToolTip>Device Built-In Self Test Status All</ToolTip>
         <Description>Return the status of the device Built-In Self Test as a bitfield. The meaning for each bit is device-specific.</Description>
         <DisplayName>Device Built-In Self Test Status All</DisplayName>
         <Visibility>Beginner</Visibility>
         <pValue>pdeviceBISTStatusAllReg</pValue>
      </Integer>
      <IntReg Name="pdeviceBISTStatusAllReg">
         <Address>0x180081AC</Address>
         <Length>4</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pdeviceBISTCmdReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Command Name="DeviceReset" NameSpace="Standard">
         <ToolTip>Reset to Power Up State</ToolTip>
         <Description>Resets the device to its power up state.</Description>
         <DisplayName>Device Reset</DisplayName>
         <Visibility>Beginner</Visibility>
         <pValue>deviceReset_Reg</pValue>
         <CommandValue>1</CommandValue>
      </Command>
      <IntReg Name="deviceReset_Reg">
         <ToolTip>Device reset register</ToolTip>
         <Address>0x18000010</Address>
         <Length>4</Length>
         <AccessMode>WO</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Integer Name="deviceCalibrationDateRaw_regOffset">
         <Value>0x180000C0</Value>
      </Integer>	  
      <Integer Name="deviceCalibrationDateRaw" NameSpace="Custom">
         <ToolTip>Date when the camera was calibrated</ToolTip>
         <Description>Date when the camera was calibrated</Description>
         <DisplayName>Calibration Date</DisplayName>
         <Visibility>Invisible</Visibility> 
         <pValue>deviceCalibrationDateRaw_reg</pValue>
      </Integer>
      <IntReg Name="deviceCalibrationDateRaw_reg" NameSpace="Custom">
         <pAddress>deviceCalibrationDateRaw_regOffset</pAddress>
         <Length>4</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
         <Representation>Linear</Representation>
      </IntReg>	  
   </Group>

