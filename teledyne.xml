      <IntSwissKnife Name="CYCLING_IS_LOCKED">
         <pVariable Name="REGISTER_STREAMING">writeStreamingState</pVariable>
         <pVariable Name="AUTO_BRIGHTNESS_MODE">pAutoBrightnessModeReg</pVariable>
         <pVariable Name="MULTISLOPE_MODE">multiSlopeModeReg</pVariable>
         <pVariable Name="FFC_MODE">pFFCCorrectionModeReg</pVariable>
         <pVariable Name="TL_PARAMS_LOCKED">TLParamsLocked</pVariable>
         <Formula>(((AUTO_BRIGHTNESS_MODE &lt;&gt; 0) || (FFC_MODE =2)) &amp;&amp; (REGISTER_STREAMING ? 0 : 1)) || TL_PARAMS_LOCKED</Formula>
      </IntSwissKnife>
      <Enumeration Name="cyclingPresetMode">
         <ToolTip>Cycling Presets module mode</ToolTip>
         <Description>Sets the Cycling Presets module mode.</Description>
         <DisplayName>Cycling Preset Mode</DisplayName>
         <Visibility>Expert</Visibility>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="Off">
            <ToolTip>Disables the Cycling Preset module</ToolTip>
            <Description>Disable the Cycling Preset module.</Description>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="Active">
            <ToolTip>Enables the Cycling Preset module</ToolTip>
            <Description>Enable the Cycling Preset module.</Description>
            <Value>1</Value>
         </EnumEntry>
         <pValue>cyclingPresetMode_reg</pValue>
      </Enumeration>
      <StructReg Comment="Register to set the mode of the preset">
         <pIsLocked>CYCLING_IS_LOCKED</pIsLocked>
         <Address>0x200057D0</Address>
         <pAddress>persistenceOffset</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pAutoBrightnessModeReg</pInvalidator>
         <pInvalidator>multiSlopeModeReg</pInvalidator>
         <Endianess>LittleEndian</Endianess>
         <StructEntry Name="cyclingPresetMode_reg">
            <Bit>31</Bit>
         </StructEntry>
         <StructEntry Name="cyclingPresetFeatureMode_reg">
            <LSB>0</LSB>
            <MSB>30</MSB>
            <Sign>Unsigned</Sign>
         </StructEntry>
      </StructReg>
      <Integer Name="cyclingPresetCount">
         <ToolTip>Number of Cycling Presets</ToolTip>
         <Description>Specifies the number of Presets to use.</Description>
         <DisplayName>Cycling Preset Count</DisplayName>
         <Visibility>Expert</Visibility>
         <Streamable>Yes</Streamable>
         <pValue>cyclingPresetCount_reg</pValue>
         <pValueCopy>cyclingPresetSelector_adjustement</pValueCopy>
         <Min>2</Min>
         <Max>6</Max>
         <Inc>1</Inc>
         <Representation>Linear</Representation>
      </Integer>
      <IntReg Name="cyclingPresetCount_reg">
         <pIsLocked>CYCLING_IS_LOCKED</pIsLocked>
         <Address>0x200057F0</Address>
         <pAddress>persistenceOffset</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="CURRENT_ACTIVE_SET_IS_LOCKED">
         <pVariable Name="SRC">cyclingPresetIncrementalSource_reg</pVariable>
         <pVariable Name="MODE">cyclingPresetMode_reg</pVariable>
         <Formula>(SRC &lt; 5) || (MODE = 0)</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="CURRENT_ACTIVE_SET_POLLING_ACTIVE">
         <pVariable Name="MODE">cyclingPresetMode_reg</pVariable>
         <Formula>(MODE = 1) ? 0 : 1</Formula>
      </IntSwissKnife>
