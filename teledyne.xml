         <pFeature>cP_FeaturesActivationSelector</pFeature>
         <pFeature>cP_FeaturesActivationMode</pFeature>
         <pFeature>cP_PresetConfigurationSelector</pFeature>
         <pFeature>cP_ExposureTime</pFeature>
         <pFeature>cP_GainSelector</pFeature>
         <pFeature>cP_Gain</pFeature>
         <pFeature>cP_BlackLevelSelector</pFeature>
         <pFeature>cP_BlackLevel</pFeature>
