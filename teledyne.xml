         <pFeature>EventImageLostData</pFeature>
         <!--<pFeature>EventexposureEndWithBlockIdData</pFeature>-->
         <pFeature>EventeventsOverflowData</pFeature>
      </Category>
   
      <Enumeration Name="timestampSource" NameSpace="Custom">
         <ToolTip>TimeStamp Source</ToolTip>
         <Description>Specifies the source used as the incrementing signal for the Timestamp register. </Description>
         <DisplayName>TimeStamp Source</DisplayName>
         <Visibility>Expert</Visibility>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="InternalClock" NameSpace="Custom">
            <ToolTip>Internal Clock</ToolTip>
            <Description>The timestamp source is generated by the camera internal clock. Refer to the timestampTickFrequency feature for the time base.</Description>
            <DisplayName>Internal Clock</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="Line3" NameSpace="Custom">
            <ToolTip>Input Line 3</ToolTip>
            <Description>Use physical line 3 and associated I/O control block to use as the timestamp increment source.</Description>
            <DisplayName>Line 3</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>4</Value>
         </EnumEntry>
         <EnumEntry Name="Line4" NameSpace="Custom">
            <ToolTip>Input Line 4</ToolTip>
            <Description>Use physical line 4 and associated I/O control block to use as the timestamp increment source.</Description>
            <DisplayName>Line 4</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>5</Value>
         </EnumEntry>
         <pValue>pTimestampSourceReg</pValue>
      </Enumeration>
      <IntReg Name="pTimestampSourceReg">
         <pIsLocked>TLParamsLocked</pIsLocked>
         <Address>0x20005320</Address>
         <pAddress>persistenceOffset</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="IsTsSourceLineSelected">
         <pVariable Name="TS_SOURCE.Entry.Line3">timestampSource</pVariable>
         <pVariable Name="TS_SOURCE.Entry.Line4">timestampSource</pVariable>
         <pVariable Name="TS_SOURCE">timestampSource</pVariable>
         <pVariable Name="REGISTER_STREAMING">writeStreamingState</pVariable>
         <Formula> ( ( TS_SOURCE = TS_SOURCE.Entry.Line3 ) || ( TS_SOURCE = TS_SOURCE.Entry.Line4 ) || REGISTER_STREAMING ) ? 1 : 0 </Formula>
      </IntSwissKnife>
      <Enumeration Name="timestampSourceLineActivation" NameSpace="Custom">
         <ToolTip>The transition of the source signal</ToolTip>
         <Description>Defines the activation mode to increment the timestamp counter on the selected line of the TimestampSource feature.</Description>
         <DisplayName>Timestamp Source Line Activation</DisplayName>
         <Visibility>Beginner</Visibility>
         <pIsAvailable>IsTsSourceLineSelected</pIsAvailable>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="RisingEdge" NameSpace="Custom">
            <Description>Specifies that the timestamp counter increment will occur on the rising edge of the source signal.</Description>
            <DisplayName>Rising Edge</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="FallingEdge" NameSpace="Custom">
            <Description>Specifies that the timestamp counter increment will occur on the falling edge of the source signal. </Description>
            <DisplayName>Falling Edge</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>1</Value>
         </EnumEntry>
         <EnumEntry Name="AnyEdge" NameSpace="Custom">
            <Description>Specifies that the timestamp counter increment will occur on the falling or rising edge of the source signal. </Description>
            <DisplayName>Any Edge</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>2</Value>
         </EnumEntry>
         <pValue>ptsIncrementActivation</pValue>
      </Enumeration>
      <IntReg Name="ptsIncrementActivation" NameSpace="Custom">
         <Visibility>Invisible</Visibility>
         <pIsLocked>TLParamsLocked</pIsLocked>
         <Address>0x200054D0</Address>
         <pAddress>persistenceOffset</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="IsTsResetLineSelected">
         <pVariable Name="TS_RESET.Entry.Line3">timestampResetSource</pVariable>
         <pVariable Name="TS_RESET.Entry.Line4">timestampResetSource</pVariable>
         <pVariable Name="TS_RESET">timestampResetSource</pVariable>
         <pVariable Name="REGISTER_STREAMING">writeStreamingState</pVariable>
         <Formula> ( ( TS_RESET = TS_RESET.Entry.Line3 ) || ( TS_RESET = TS_RESET.Entry.Line4 )|| REGISTER_STREAMING ) ? 1 : 0 </Formula>
      </IntSwissKnife>
      <Enumeration Name="timestampResetLineActivation" NameSpace="Custom">
         <ToolTip>The transition of the source signal</ToolTip>
         <Description>Specifies the activation mode to reset the timestamp counter on the selected line of the TimestampResetSource feature.</Description>
         <DisplayName>Timestamp Reset Line Activation</DisplayName>
         <Visibility>Beginner</Visibility>
         <pIsAvailable>IsTsResetLineSelected</pIsAvailable>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="RisingEdge" NameSpace="Custom">
            <Description>Reset the timestamp counter on the rising edge of the source signal.</Description>
            <DisplayName>Rising Edge</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="FallingEdge" NameSpace="Custom">
            <Description>Reset the timestamp counter on the falling edge of the source signal.</Description>
            <DisplayName>Falling Edge</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>1</Value>
         </EnumEntry>
         <EnumEntry Name="AnyEdge" NameSpace="Custom">
            <Description>Reset the timestamp counter on the falling or rising edge of the source signal.</Description>
            <DisplayName>Any Edge</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>2</Value>
         </EnumEntry>
         <pValue>ptsResetActivation</pValue>
      </Enumeration>
      <IntReg Name="ptsResetActivation" NameSpace="Custom">
         <Visibility>Invisible</Visibility>
         <pIsLocked>TLParamsLocked</pIsLocked>
         <Address>0x200054E0</Address>
         <pAddress>persistenceOffset</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Integer Name="timestampValue" NameSpace="Custom">
         <ToolTip>Timestamp Value</ToolTip>
         <Description>Returns the 64-bit value of the timestamp counter.</Description>
         <DisplayName>Timestamp Value</DisplayName>
         <Visibility>Expert</Visibility>
         <pIsAvailable>IsTimestampAvailable</pIsAvailable>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <pAlias>GevTimestampValue</pAlias>
         <pValue>ptimestampValueReg</pValue>
      </Integer>
      <IntReg Name="ptimestampValueReg">
         <Visibility>Invisible</Visibility>
         <Address>0x20005340</Address>
         <Length>8</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pGevtimestampControlReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Integer Name="GevTimestampValue" NameSpace="Standard">
         <ToolTip>Returns the 64-bit value of the timestamp counter.</ToolTip>
         <Description>Returns the 64-bit value of the timestamp counter.</Description>
         <DisplayName>Gev Timestamp Value</DisplayName>
         <Visibility>Invisible</Visibility>
         <pIsAvailable>IsTimestampAvailable</pIsAvailable>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <pValue>GevtimestampValueLatchedCalc</pValue>
      </Integer>
      <IntReg Name="BootStrap_GevTimestampValueLatchedHigh">
         <Description>Current Gev Timestamp Value Latched High Register</Description>
         <Visibility>Invisible</Visibility>
         <Address>0x0948</Address>
         <Length>4</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pGevtimestampControlReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>BigEndian</Endianess>
      </IntReg>
      <IntReg Name="BootStrap_GevTimestampValueLatchedLow">
         <Description>Current Gev Timestamp Value Latched Low Register</Description>
         <Visibility>Invisible</Visibility>
         <Address>0x094C</Address>
         <Length>4</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pGevtimestampControlReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>BigEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="GevtimestampValueLatchedCalc">
