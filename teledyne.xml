   <!-- ================  deviceSerialPortControl  ================ -->
   <Group Comment="deviceSerialPortControl">
      <Category Name="deviceSerialPortControl" NameSpace="Custom">
         <ToolTip>Category of features the Serial Port on the device.</ToolTip>
         <DisplayName>Serial Port Control</DisplayName>
         <pFeature>DeviceSerialPortSelector</pFeature>
         <pFeature>deviceSerialPortControlMode</pFeature>
         <pFeature>deviceSerialPortSignaling</pFeature>
         <pFeature>DeviceSerialPortBaudRate</pFeature>
         <pFeature>deviceSerialPortParity</pFeature>
         <pFeature>deviceSerialPortDataSize</pFeature>
         <pFeature>deviceSerialPortNumberOfStopBits</pFeature>
      </Category>
   
      <Enumeration Name="DeviceSerialPortSelector" NameSpace="Standard">
         <ToolTip>Serial Port Selector</ToolTip>
         <Description>Selects the serial port to control.</Description>
         <DisplayName>Serial Port Selector</DisplayName>
         <Visibility>Guru</Visibility>
         <EnumEntry Name="SerialPort1">
            <ToolTip>Serial Port 1</ToolTip>
            <Description>First Serial Port available on the device.</Description>
            <DisplayName>SerialPort1</DisplayName>
            <Visibility>Guru</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="SerialPort2">
            <ToolTip>Serial Port2 </ToolTip>
            <Description>Second Serial Port available on the device.</Description>
            <DisplayName>SerialPort2</DisplayName>
            <Visibility>Guru</Visibility>
            <Value>1</Value>
         </EnumEntry>
         <pValue>deviceSerialPortVal</pValue>
         <!-- good way to impl selectors : XML ONLY -->
         <pSelected>deviceSerialPortControlMode</pSelected>
         <pSelected>deviceSerialPortSignaling</pSelected>
         <pSelected>DeviceSerialPortBaudRate</pSelected>
         <pSelected>deviceSerialPortParity</pSelected>
         <pSelected>deviceSerialPortDataSize</pSelected>
         <pSelected>deviceSerialPortNumberOfStopBits</pSelected>
      </Enumeration>
      <Integer Name="deviceSerialPortVal" NameSpace="Custom">
         <Value>0</Value>
      </Integer>
      <Enumeration Name="deviceSerialPortControlMode" NameSpace="Custom">
         <ToolTip>Serial Port Control</ToolTip>
         <Description>Specifies whether the device serial port is controlled by the device itself or remotely controlled by the host computer. </Description>
         <DisplayName>Serial Port Control</DisplayName>
         <Visibility>Expert</Visibility>
         <EnumEntry Name="RemoteHostControlled">
            <ToolTip>Remote Host Controlled</ToolTip>
            <Description>Local serial port is controlled by the host computer.</Description>
            <DisplayName>Remote Host Controlled</DisplayName>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="InCameraControlled">
            <ToolTip>In Camera Controlled</ToolTip>
            <Description>Local serial port is controlled by the camera itself.</Description>
            <DisplayName>In Camera Controlled</DisplayName>
            <Value>1</Value>
         </EnumEntry>
         <pValue>pDeviceSerialPortControlModeReg</pValue>
         <pSelected>deviceSerialPortSignaling</pSelected>
         <pSelected>DeviceSerialPortBaudRate</pSelected>
         <pSelected>deviceSerialPortParity</pSelected>
         <pSelected>deviceSerialPortDataSize</pSelected>
         <pSelected>deviceSerialPortNumberOfStopBits</pSelected>
      </Enumeration>
      <IntReg Name="pDeviceSerialPortControlModeReg">
         <Address>0x200058BC</Address>
         <pIndex Offset="0x10">deviceSerialPortVal</pIndex>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Cachable>NoCache</Cachable>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="deviceSerialPortControlMode_isRemote">
         <Visibility>Invisible</Visibility>
         <pVariable Name="CONTROLMODE">deviceSerialPortControlMode</pVariable>
         <pVariable Name="CONTROLMODE.Entry.RemoteHostControlled">deviceSerialPortControlMode</pVariable>
         <Formula>CONTROLMODE = CONTROLMODE.Entry.RemoteHostControlled</Formula>
      </IntSwissKnife>
      <Enumeration Name="deviceSerialPortSignaling" NameSpace="Custom">
         <ToolTip>Serial Port Signaling </ToolTip>
         <Description>Displays the current serial port signaling protocol in use by the device. This feature selects the protocol if multiple types are supported.</Description>
         <DisplayName>Serial Port Signaling </DisplayName>
         <Visibility>Expert</Visibility>
         <pIsLocked>deviceSerialPortControlMode_isRemote</pIsLocked>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <EnumEntry Name="RS232">
            <ToolTip>RS-232</ToolTip>
            <Description>Use RS-232 signaling protocol.</Description>
            <DisplayName>RS-232</DisplayName>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="RS485">
            <ToolTip>RS-485</ToolTip>
            <Description>Use RS-485 signaling protocol.</Description>
            <DisplayName>RS-485</DisplayName>
            <Value>1</Value>
         </EnumEntry>
         <pValue>deviceSerialPortSignalingVal</pValue>
      </Enumeration>
      <IntSwissKnife Name="deviceSerialPortSignalingVal">
         <Visibility>Invisible</Visibility>
         <pVariable Name="SERIALPORTSELECTOR">deviceSerialPortVal</pVariable>
         <pVariable Name="SERIALPORTSELECTOR.Entry.SerialPort1">DeviceSerialPortSelector</pVariable>
         <Formula>(SERIALPORTSELECTOR = SERIALPORTSELECTOR.Entry.SerialPort1) ? 0 : 1</Formula>
      </IntSwissKnife>
      <Enumeration Name="DeviceSerialPortBaudRate" NameSpace="Standard">
         <ToolTip>Serial Port Baud Rate </ToolTip>
         <Description>Sets the baud rate used by the selected device’s serial port. Available baud rates are device-specific.</Description>
         <DisplayName>Serial Port Baud Rate </DisplayName>
         <Visibility>Expert</Visibility>
         <pIsLocked>deviceSerialPortControlMode_isRemote</pIsLocked>
         <EnumEntry Name="Baud9600">
            <ToolTip>Baud 9600</ToolTip>
            <Description>Baud rate is 9600.</Description>
            <DisplayName>Baud 9600</DisplayName>
            <Value>9600</Value>
         </EnumEntry>
         <EnumEntry Name="Baud115200">
            <ToolTip>Baud 115200</ToolTip>
            <Description>Baud rate is 115200.</Description>
            <DisplayName>Baud 115200</DisplayName>
            <Value>115200</Value>
         </EnumEntry>
         <pValue>pDeviceSerialPortBaudRateReg</pValue>
      </Enumeration>
      <IntReg Name="pDeviceSerialPortBaudRateReg">
         <Address>0x200058DC</Address>
         <pIndex Offset="0x10">deviceSerialPortVal</pIndex>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Cachable>NoCache</Cachable>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Enumeration Name="deviceSerialPortParity" NameSpace="Custom">
         <ToolTip>Serial Port Parity </ToolTip>
         <Description>Sets the parity checking type on the selected serial port.</Description>
         <DisplayName>Serial Port Parity </DisplayName>
         <Visibility>Guru</Visibility>
         <pIsLocked>deviceSerialPortControlMode_isRemote</pIsLocked>
         <EnumEntry Name="Even">
            <ToolTip>Even</ToolTip>
            <Description>Use Even parity checking.</Description>
            <DisplayName>Even</DisplayName>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="Odd">
            <ToolTip>Odd</ToolTip>
            <Description>Use Odd parity checking.</Description>
            <DisplayName>Odd</DisplayName>
            <Value>1</Value>
         </EnumEntry>
         <EnumEntry Name="None">
            <ToolTip>None</ToolTip>
            <Description>Parity checking is disabled.</Description>
            <DisplayName>None</DisplayName>
            <Value>2</Value>
         </EnumEntry>
         <pValue>pDeviceSerialPortParityReg</pValue>
      </Enumeration>
      <IntReg Name="pDeviceSerialPortParityReg">
         <Address>0x200058FC</Address>
         <pIndex Offset="0x10">deviceSerialPortVal</pIndex>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Cachable>NoCache</Cachable>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Enumeration Name="deviceSerialPortDataSize" NameSpace="Custom">
         <ToolTip>Serial Port Data Size</ToolTip>
         <Description>Sets the bits per character (bpc) to use.</Description>
         <DisplayName>Serial Port Data Size</DisplayName>
         <Visibility>Guru</Visibility>
         <pIsLocked>deviceSerialPortControlMode_isRemote</pIsLocked>
         <EnumEntry Name="bpc8">
            <ToolTip>bpc8</ToolTip>
            <Description>Use 8 bits per character</Description>
            <DisplayName>bpc8</DisplayName>
            <Value>8</Value>
         </EnumEntry>
         <EnumEntry Name="bpc7">
            <ToolTip>bpc7</ToolTip>
            <Description>Use 7 bits per character</Description>
            <DisplayName>bpc7</DisplayName>
            <Value>7</Value>
         </EnumEntry>
         <pValue>pDeviceSerialPortDataSizeReg</pValue>
      </Enumeration>
      <IntReg Name="pDeviceSerialPortDataSizeReg">
         <Address>0x2000591C</Address>
         <pIndex Offset="0x10">deviceSerialPortVal</pIndex>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Cachable>NoCache</Cachable>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Enumeration Name="deviceSerialPortNumberOfStopBits" NameSpace="Custom">
         <ToolTip>Serial Port Number of Stop Bits</ToolTip>
         <Description>Sets the number of stop bits to use.</Description>
         <DisplayName>Serial Port Number of Stop Bits</DisplayName>
         <Visibility>Guru</Visibility>
         <pIsLocked>deviceSerialPortControlMode_isRemote</pIsLocked>
         <EnumEntry Name="Stopbits0">
            <ToolTip>Stopbits0</ToolTip>
            <Description>Use no stop bit.</Description>
            <DisplayName>Stopbits0</DisplayName>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="Stopbits1">
            <ToolTip>Stopbits1</ToolTip>
            <Description>Use 1 stop bit.</Description>
            <DisplayName>Stopbits1</DisplayName>
            <Value>1</Value>
         </EnumEntry>
         <EnumEntry Name="Stopbits2">
            <ToolTip>Stopbits2</ToolTip>
            <Description>Use 2 stop bits.</Description>
            <DisplayName>Stopbits2</DisplayName>
            <Value>2</Value>
         </EnumEntry>
         <pValue>pDeviceSerialPortNumberOfStopBitsReg</pValue>
      </Enumeration>
      <IntReg Name="pDeviceSerialPortNumberOfStopBitsReg">
         <Address>0x2000593C</Address>
         <pIndex Offset="0x10">deviceSerialPortVal</pIndex>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Cachable>NoCache</Cachable>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
   </Group>

