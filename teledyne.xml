            <ToolTip>Line 1</ToolTip>
            <Description>Select counter 1</Description>
            <DisplayName>Counter 1</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <pValue>counterSelectorVal</pValue>
         <pSelected>counterMode</pSelected>
         <pSelected>counterStatus</pSelected>
         <pSelected>counterStartSource</pSelected>
         <pSelected>counterStartLineActivation</pSelected>
         <pSelected>counterIncrementalSource</pSelected>
         <pSelected>counterIncrementalLineActivation</pSelected>
         <pSelected>counterDuration</pSelected>
         <pSelected>counterResetSource</pSelected>
         <pSelected>counterResetLineActivation</pSelected>
         <pSelected>counterValue</pSelected>
         <pSelected>counterValueAtReset</pSelected>
         <pSelected>counterReset</pSelected>
