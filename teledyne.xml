   <!-- ================  ImageFormatControl  ================ -->
   <Group Comment="ImageFormatControl">
      <Category Name="ImageFormatControl" NameSpace="Standard">
         <ToolTip>Features controlling the size of the transmitted image</ToolTip>
         <DisplayName>Image Format Controls</DisplayName>
         <pFeature>dataStreamSelector</pFeature>
         <pFeature>dataStreamType</pFeature>
         <pFeature>PixelFormat</pFeature>
         <pFeature>PixelCoding</pFeature>
         <pFeature>PixelColorFilter</pFeature>
         <pFeature>PixelSize</pFeature>
         <pFeature>WidthMax</pFeature>
         <pFeature>HeightMax</pFeature>
         <pFeature>OffsetX</pFeature>
         <pFeature>OffsetY</pFeature>
         <pFeature>Width</pFeature>
         <pFeature>Height</pFeature>
         <pFeature>binningSelector</pFeature>
         <pFeature>BinningHorizontal</pFeature>
         <pFeature>BinningVertical</pFeature>
         <!-- <pFeature>ReverseX</pFeature>-->
         <!-- <pFeature>ReverseY</pFeature>-->
         <pFeature>TestImageSelector</pFeature>
         <pFeature>testImageUserDefinedCyclingMode</pFeature>
         <pFeature>testImageUserDefinedIndex</pFeature>
         <pFeature>testImageUserDefinedWidth</pFeature>
         <pFeature>testImageUserDefinedHeight</pFeature>
      </Category>
   
      <IntSwissKnife Name="IS_METADATA_ONLY_IMPLEMENTED">
         <Formula>0</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="IS_JPEG_IMPLEMENTED">
         <Formula>0</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="WIDTH_MAX_RAW">
         <pVariable Name="SENSOR_WIDTH">pSensorWidthReg</pVariable>
         <pVariable Name="OFFSETX">OffsetX_Biggest_Reg</pVariable>
         <Formula>SENSOR_WIDTH - OFFSETX</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="OFFSETX_MAX_RAW">
         <pVariable Name="SENSOR_WIDTH">pSensorWidthReg</pVariable>
         <pVariable Name="WIDTH">pWidthReg</pVariable>
         <Formula> SENSOR_WIDTH - WIDTH </Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="WIDTH_MIN">
         <pVariable Name="PIXEL_SIZE">PixelSizeVal</pVariable>
         <pVariable Name="BINNING">pBinningHorizontalReg</pVariable>
         <Formula>32/ BINNING </Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="WIDTH_INC">
         <pVariable Name="PIXEL_SIZE">PixelSizeVal</pVariable>
         <pVariable Name="BINNING">pBinningHorizontalReg</pVariable>
         <Formula>(BINNING = 1)? 8: (8 / (PIXEL_SIZE / 8))</Formula>
      </IntSwissKnife>
      <Integer Name="OFFSETX_INC" NameSpace="Custom">
         <Value>4</Value>
      </Integer>
      <Integer Name="Width" NameSpace="Standard">
         <ToolTip>Width (in pixels) of the image</ToolTip>
         <Description>Width of the Image provided by the device (in pixels).</Description>
         <DisplayName>Width</DisplayName>
         <Visibility>Beginner</Visibility>
         <Streamable>Yes</Streamable>
         <pValue>width_user_space</pValue>
         <pMin>WIDTH_MIN</pMin>
         <pInc>WIDTH_INC</pInc>
      </Integer>
      <IntConverter Name="width_user_space">
         <pVariable Name="BINNING">pBinningHorizontalReg</pVariable>
         <pVariable Name="REG_STREAMING">writeStreamingState</pVariable>
         <FormulaTo>FROM* ((REG_STREAMING)? 1:BINNING)</FormulaTo>
         <FormulaFrom>TO/ ((REG_STREAMING)? 1:BINNING)</FormulaFrom>
         <pValue>width_raw</pValue>
         <Representation>Linear</Representation>
         <Slope>Increasing</Slope>
      </IntConverter>
      <Integer Name="width_raw">
         <pValue>pWidthReg</pValue>
         <pMax>WIDTH_MAX_RAW</pMax>
      </Integer>
      <IntReg Name="pWidthReg">
         <ToolTip>Width of image/area of interest register</ToolTip>
         <pIsLocked>FFC_CALIBRATION_OR_TL_PARAMS_LOCKED</pIsLocked>
         <Address>0x2000006C</Address>
         <pAddress>writeStreamingState</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pOffsetXReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Integer Name="OffsetX" NameSpace="Standard">
         <ToolTip>OffsetX from Sensor Origin</ToolTip>
         <Description>Horizontal offset from the Sensor Origin to the Area Of Interest (in pixels).</Description>
         <DisplayName>Horizontal Offset</DisplayName>
         <Visibility>Beginner</Visibility>
         <Streamable>Yes</Streamable>
         <pValue>offsetx_convertor</pValue>
         <pInc>OFFSETX_INC</pInc>
      </Integer>
      <IntConverter Name="offsetx_convertor">
         <Visibility>Invisible</Visibility>
         <pVariable Name="BINNING">pBinningHorizontalReg</pVariable>
         <pVariable Name="REG_STREAMING">writeStreamingState</pVariable>
         <FormulaTo>FROM* ((REG_STREAMING)? 1:BINNING)</FormulaTo>
         <FormulaFrom>TO/ ((REG_STREAMING)? 1:BINNING)</FormulaFrom>
         <pValue>offsetx_raw</pValue>
         <Representation>Linear</Representation>
         <Slope>Increasing</Slope>
      </IntConverter>
      <Integer Name="offsetx_raw">
         <pValue>pOffsetXReg</pValue>
         <pMax>OFFSETX_MAX_RAW</pMax>
      </Integer>
      <IntSwissKnife Name="Offsets_Locked">
         <pVariable Name="FFC_CALIB">FFC_IS_CALIBRATION_MODE</pVariable>
         <pVariable Name="TL_PARAMS_LOCKED">TLParamsLocked</pVariable>
         <pVariable Name="CP_ROI_POSITION_ACTIVE">cP_ROIPosition_Active</pVariable>
         <Formula>CP_ROI_POSITION_ACTIVE || FFC_CALIB || TL_PARAMS_LOCKED</Formula>
      </IntSwissKnife>
      <IntReg Name="pOffsetXReg">
         <ToolTip>AOI Left register</ToolTip>
         <pIsLocked>Offsets_Locked</pIsLocked>
         <Address>0x200000CC</Address>
         <pAddress>writeStreamingState</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>cP_OffsetX_Reg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntReg Name="OffsetX_Biggest_Reg">
         <Address>0x2000636C</Address>
         <Length>4</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pOffsetXReg</pInvalidator>
         <pInvalidator>cP_OffsetX_Reg</pInvalidator>
         <pInvalidator>cP_ROIPosition_Active</pInvalidator>
         <pInvalidator>cyclingPresetCount_reg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="HEIGHT_MAX_RAW">
         <pVariable Name="SENSOR_HEIGHT">pSensorHeightReg</pVariable>
         <pVariable Name="OFFSETY">OffsetY_Biggest_Reg</pVariable>
         <Formula>SENSOR_HEIGHT - OFFSETY</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="HEIGHT_INC">
         <pVariable Name="BINNING">pBinningVerticalReg</pVariable>
         <Formula>(BINNING = 1)? 2: 1</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="HEIGHT_MIN">
         <pVariable Name="BINNING">pBinningVerticalReg</pVariable>
         <Formula>(BINNING = 1)? 2: 1</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="OFFSETY_MAX_RAW">
         <pVariable Name="SENSOR_HEIGHT">pSensorHeightReg</pVariable>
         <pVariable Name="HEIGHT">pHeightReg</pVariable>
         <Formula>SENSOR_HEIGHT - HEIGHT</Formula>
      </IntSwissKnife>
      <Integer Name="Height" NameSpace="Standard">
         <ToolTip>Height (in pixels) of the image</ToolTip>
         <Description>Height of the Image provided by the device (in lines).</Description>
         <DisplayName>Height</DisplayName>
         <Visibility>Beginner</Visibility>
         <Streamable>Yes</Streamable>
         <pValue>height_convertor</pValue>
         <pMin>HEIGHT_MIN</pMin>
         <pInc>HEIGHT_INC</pInc>
      </Integer>
      <IntConverter Name="height_convertor" NameSpace="Custom">
         <ToolTip>Height raw (in pixels) of the image</ToolTip>
         <Description>Height raw of the Image provided by the device (in pixels).</Description>
         <DisplayName>Height raw</DisplayName>
         <Visibility>Invisible</Visibility>
         <pVariable Name="BINNING">pBinningVerticalReg</pVariable>
         <pVariable Name="REG_STREAMING">writeStreamingState</pVariable>
         <FormulaTo>FROM* ((REG_STREAMING)? 1:BINNING)</FormulaTo>
         <FormulaFrom>TO/ ((REG_STREAMING)? 1:BINNING)</FormulaFrom>
         <pValue>height_raw</pValue>
         <Representation>Linear</Representation>
         <Slope>Increasing</Slope>
      </IntConverter>
      <Integer Name="height_raw">
         <pValue>pHeightReg</pValue>
         <pMax>HEIGHT_MAX_RAW</pMax>
      </Integer>
      <IntReg Name="pHeightReg">
         <ToolTip>Height of image/area of interest register</ToolTip>
         <pIsLocked>FFC_CALIBRATION_OR_TL_PARAMS_LOCKED</pIsLocked>
         <Address>0x2000012C</Address>
         <pAddress>writeStreamingState</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pOffsetYReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <Integer Name="OffsetY" NameSpace="Standard">
         <ToolTip>OffsetY from Sensor Origin.</ToolTip>
         <Description>Vertical offset from the Sensor Origin to the Area Of Interest (in Lines).</Description>
         <DisplayName>Vertical Offset</DisplayName>
         <Visibility>Beginner</Visibility>
         <Streamable>Yes</Streamable>
         <pValue>offsety_convertor</pValue>
         <Min>0</Min>
         <pInc>offsetY_Increment</pInc>
      </Integer>
      <IntSwissKnife Name="offsetY_Increment">
         <pVariable Name="SENSOR_COLOR_TYPE">sensorColorType</pVariable>
         <pVariable Name="SENSOR_COLOR_TYPE.Entry.CFA_Bayer">sensorColorType</pVariable>
         <Formula>SENSOR_COLOR_TYPE = SENSOR_COLOR_TYPE.Entry.CFA_Bayer ? 2 : 1</Formula>
      </IntSwissKnife>
      <IntConverter Name="offsety_convertor">
         <Visibility>Invisible</Visibility>
         <pVariable Name="BINNING">pBinningVerticalReg</pVariable>
         <pVariable Name="REG_STREAMING">writeStreamingState</pVariable>
         <FormulaTo>FROM* ((REG_STREAMING)? 1:BINNING)</FormulaTo>
         <FormulaFrom>TO/ ((REG_STREAMING)? 1:BINNING)</FormulaFrom>
         <pValue>offsety_raw</pValue>
         <Representation>Linear</Representation>
         <Slope>Increasing</Slope>
      </IntConverter>
      <Integer Name="offsety_raw">
         <pValue>pOffsetYReg</pValue>
         <pMax>OFFSETY_MAX_RAW</pMax>
      </Integer>
      <IntReg Name="pOffsetYReg">
         <ToolTip>AOI Top register</ToolTip>
         <pIsLocked>Offsets_Locked</pIsLocked>
         <Address>0x2000018C</Address>
         <pAddress>writeStreamingState</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>cP_OffsetY_Reg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntReg Name="OffsetY_Biggest_Reg">
         <Address>0x2000637C</Address>
         <Length>4</Length>
         <AccessMode>RO</AccessMode>
         <pPort>Device</pPort>
         <pInvalidator>pOffsetYReg</pInvalidator>
         <pInvalidator>cP_OffsetY_Reg</pInvalidator>
         <pInvalidator>cP_ROIPosition_Active</pInvalidator>
         <pInvalidator>cyclingPresetCount_reg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="isBpp8Implemented" NameSpace="Custom">
         <pVariable Name="MONO8">isPixelFormatMono8Implemented</pVariable>
         <pVariable Name="BAYER8">isPixelFormatBayer8Implemented</pVariable>
         <Formula>MONO8 || BAYER8</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="isBpp10Implemented" NameSpace="Custom">
         <pVariable Name="MONO10">isPixelFormatMono10Implemented</pVariable>
         <pVariable Name="BAYER10">isPixelFormatBayer10Implemented</pVariable>
         <Formula>MONO10 || BAYER10</Formula>
      </IntSwissKnife>
      <IntSwissKnife Name="isBpp12Implemented" NameSpace="Custom">
         <pVariable Name="MONO12">isPixelFormatMono12Implemented</pVariable>
         <pVariable Name="BAYER12">isPixelFormatBayer12Implemented</pVariable>
         <Formula>MONO12 || BAYER12</Formula>
      </IntSwissKnife>
      <Enumeration Name="dataStreamSelector">
         <ToolTip>Select which data stream to control (default is Stream 1) </ToolTip>
         <DisplayName>Data Stream Selector </DisplayName>
         <Visibility>Expert</Visibility>
         <EnumEntry Name="Stream1">
            <ToolTip>Stream1</ToolTip>
            <Description>Adjust parameters for Stream1.</Description>
            <DisplayName>Stream1</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <pValue>dataStreamSelector_Value</pValue>
         <pSelected>dataStreamType</pSelected>
      </Enumeration>
      <Integer Name="dataStreamSelector_Value">
         <ImposedAccessMode>RO</ImposedAccessMode>
         <Value>0</Value>
      </Integer>
      <Enumeration Name="dataStreamType">
