      </Integer>
      <IntSwissKnife Name="gainAddr">
         <pVariable Name="GAIN_SELECTOR">gainSelectorInt</pVariable>
         <pVariable Name="ANALOG_GAIN">analogCoarseGainRegAddr</pVariable>
         <pVariable Name="DIGITAL_GAIN">digitalGainRegAddr</pVariable>
         <Formula>(GAIN_SELECTOR)?  DIGITAL_GAIN: ANALOG_GAIN</Formula>
      </IntSwissKnife>
