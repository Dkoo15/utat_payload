      <Enumeration Name="multiSlopeKneeSelector" NameSpace="Custom">
         <ToolTip>MultiSlope Knee Selector</ToolTip>
         <Description>Selects which mutlislope knee position to set.</Description>
         <DisplayName>MultiSlope Knee Selector</DisplayName>
         <Visibility>Expert</Visibility>
         <pIsAvailable>isMultiSlopeEnabled</pIsAvailable>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <EnumEntry Name="KneePoint1" NameSpace="Custom">
            <ToolTip>Selects the first knee point</ToolTip>
            <Description>Selects the first knee point.</Description>
            <DisplayName>Knee Point 1</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <Value>0</Value>
      </Enumeration>
      <Float Name="multiSlopeKneePositionY" NameSpace="Custom">
         <ToolTip>MultiSlope Knee Position Y</ToolTip>
         <Description>Sets the MultiSlope Knee position as a % of the saturated luminance output.</Description>
         <DisplayName>MultiSlope Knee Position Y</DisplayName>
         <Visibility>Expert</Visibility>
         <pIsAvailable>isMultiSlopeEnabled</pIsAvailable>
         <Streamable>Yes</Streamable>
         <pValue>pMultiSlopeKneePositionYConv</pValue>
         <Min>63.0</Min>
         <!-- todo: move to camera registers -->
         <Max>100.0</Max>
         <!-- todo: move to camera registers -->
         <Inc>1.0</Inc>
      </Float>
      <Converter Name="pMultiSlopeKneePositionYConv" NameSpace="Custom">
         <FormulaTo>(FROM = 100) ? 20 : (
                    (FROM = 99) ? 60 : (
                    ((FROM &lt; 99) &amp;&amp; (FROM &gt; 65)) ? ROUND((180.5 - (FROM * 1.22)), 0) : (
                    (FROM = 65) ? 105 : (
                    (FROM = 64) ? 110 : (
                    (FROM = 63) ? 120 : 20 ) ) ) ) ) 
         </FormulaTo>
         <FormulaFrom>(TO = 20) ? 100 : (
                      (TO = 60) ? 99 : (
                      ((TO &lt; 105) &amp;&amp; (TO &gt; 60)) ? ROUND(((180.5 - TO) / 1.22), 0) : (
                      (TO = 105) ? 65 : (
                      (TO = 110) ? 64 : (
                      (TO = 120) ? 63 : 100 ) ) ) ) )
         </FormulaFrom>
         <pValue>pMultiSlopeKneePositionY_reg</pValue>
         <Slope>Varying</Slope>
         <IsLinear>No</IsLinear>
      </Converter>
      <IntReg Name="pMultiSlopeKneePositionY_reg" NameSpace="Custom">
         <pIsLocked>TLParamsLocked</pIsLocked>
         <Address>0x20005BAC</Address>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Sign>Signed</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
