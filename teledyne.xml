      <Enumeration Name="flatfieldCorrectionMode" NameSpace="Custom">
         <ToolTip>Flat Field Correction Mode</ToolTip>
         <Description>Sets the mode for the Flat Field correction.</Description>
         <DisplayName>Flat Field Correction Mode</DisplayName>
         <Visibility>Beginner</Visibility>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="Off" NameSpace="Custom">
            <ToolTip>Off</ToolTip>
            <Description>Flat Field Correction is disabled.</Description>
            <DisplayName>Off</DisplayName>
            <Visibility>Beginner</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="Active" NameSpace="Custom">
            <ToolTip>Active</ToolTip>
            <Description>Flat Field Correction is enabled.</Description>
            <DisplayName>Active</DisplayName>
            <Visibility>Beginner</Visibility>
            <pIsAvailable>FFC_ACTIVE_MODE_AVAILABLE</pIsAvailable>
            <Value>1</Value>
         </EnumEntry>
         <EnumEntry Name="Calibration" NameSpace="Custom">
            <ToolTip>Calibrate Flat Field</ToolTip>
            <Description>When this mode is selected, the camera is configured for flat field correction calibration. The device may automatically adjust some of its features when calibrate mode is enabled. The features that are automatically adjusted are device specific. The device will not restore these features when the Flat Field Correction Mode feature is changed from Calibrate mode to another mode.</Description>
            <DisplayName>Calibration</DisplayName>
            <Visibility>Expert</Visibility>
            <pIsAvailable>FFC_CALIBRATION_MODE_AVAILABLE</pIsAvailable>
            <Value>2</Value>
         </EnumEntry>
         <pValue>ffcMode</pValue>
      </Enumeration>
      <IntConverter Name="ffcMode">
         <pIsLocked>cp_FFC_Available</pIsLocked>
         <pVariable Name="REGISTER_STREAMING">registerStreamingState</pVariable>
         <FormulaTo>FROM</FormulaTo>
         <FormulaFrom>(REGISTER_STREAMING &amp;&amp; (TO &gt; 1))?0:TO</FormulaFrom>
         <pValue>pFFCCorrectionModeReg</pValue>
         <Slope>Varying</Slope>
      </IntConverter>
