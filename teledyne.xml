         <Formula>(CP_GAIN) || ((REGISTER_STREAMING ? 0 : 1) &amp;&amp; (( (AUTO_BRIGHTNESS_MODE = 1) &amp;&amp; (GAIN_AUTO_MODE &lt;&gt; 0) &amp;&amp; (GAIN_SELECTOR = 1) ) || ((MULTISLOPE_MODE = 1) &amp;&amp;(GAIN_SELECTOR = 0))))</Formula>
      </IntSwissKnife>
