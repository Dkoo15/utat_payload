         <pInvalidator>fileOperationExecute_Reg</pInvalidator>
         <pInvalidator>cp_pFFCCorrectionModeReg</pInvalidator>
         <pInvalidator>multiSlopeModeReg</pInvalidator>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
