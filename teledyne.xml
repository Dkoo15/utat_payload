         <DisplayName>DFNC Major Rev</DisplayName>
         <Visibility>Invisible</Visibility>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <Value>1</Value>
      </Integer>
      <Integer Name="deviceDFNCVersionMinor" NameSpace="Custom">
