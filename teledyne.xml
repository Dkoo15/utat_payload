         <Description>Selects the counter mode. The selected Counter is Active or Disabled. When Disabled, the Counter can be configured</Description>
         <DisplayName>Counter mode</DisplayName>
         <Visibility>Expert</Visibility>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="Off">
            <ToolTip>The selected Counter is Disable.</ToolTip>
            <Description>The selected Counter is Disable. </Description>
            <DisplayName>Off</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="Active">
            <ToolTip>The selected Counter is Active</ToolTip>
            <Description>The selected Counter is Enable.</Description>
            <DisplayName>Active</DisplayName>
            <Visibility>Expert</Visibility>
            <Value>1</Value>
         </EnumEntry>
         <pValue>pCounterModeReg</pValue>
      </Enumeration>
