   <!-- ================  deviceInformation  ================ -->
   <Group Comment="deviceInformation">
      <Category Name="deviceInformation" NameSpace="Custom">
         <ToolTip>Description of the camera</ToolTip>
         <DisplayName>Camera Information</DisplayName>
