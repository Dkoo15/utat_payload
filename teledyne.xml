      <IntSwissKnife Name="pIsOutputLinePulseActivation">
         <Visibility>Invisible</Visibility>
         <pVariable Name="OUTPUT_LINE_SOURCE">outputLineSource</pVariable>
         <pVariable Name="SOURCE.Entry.PulseOnInput1">outputLineSource</pVariable>
         <pVariable Name="SOURCE.Entry.PulseOnInput2">outputLineSource</pVariable>
         <pVariable Name="SOURCE.Entry.PulseOnInput3">outputLineSource</pVariable>
         <pVariable Name="SOURCE.Entry.PulseOnInput4">outputLineSource</pVariable>
         <pVariable Name="REGISTER_STREAMING">writeStreamingState</pVariable>
         <Formula>(OUTPUT_LINE_SOURCE = SOURCE.Entry.PulseOnInput1) || (OUTPUT_LINE_SOURCE = SOURCE.Entry.PulseOnInput2) || (OUTPUT_LINE_SOURCE = SOURCE.Entry.PulseOnInput3) || (OUTPUT_LINE_SOURCE = SOURCE.Entry.PulseOnInput4) || REGISTER_STREAMING</Formula>
      </IntSwissKnife>
