         <Visibility>Guru</Visibility>
         <ImposedAccessMode>RO</ImposedAccessMode>
         <EnumEntry Name="Method1" NameSpace="Custom">
            <ToolTip>Method 1</ToolTip>
            <Description>The following formula is used to calculate the flat field corrected pixel: newPixelValue[x][y] = (sensorPixelValue[x][y] - FFCOffset[x][y]) * FFCGain[x][y]</Description>
            <DisplayName>Method 1</DisplayName>
            <Visibility>Guru</Visibility>
            <Value>0</Value>
         </EnumEntry>
         <Value>0</Value>
      </Enumeration>
