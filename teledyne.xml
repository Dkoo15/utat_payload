      </Integer>
      <Enumeration Name="TransferControlMode" NameSpace="Standard">
         <ToolTip>Control method for transfer</ToolTip>
         <Description>Sets the method used to control the transfer.</Description>
         <DisplayName>Transfer Control</DisplayName>
         <Visibility>Expert</Visibility>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="Basic">
            <ToolTip>The transfer is not contolled</ToolTip>
            <Description>Basic mode ensures maximum compatibility but does not allow for control of the transfer flow. </Description>
            <Value>2</Value>
         </EnumEntry>
         <EnumEntry Name="UserControlled">
            <ToolTip>Transfer flow is controlled by the user</ToolTip>
            <Description>Manual mode allows maximum control of the transfer flow.</Description>
            <DisplayName>User Controlled</DisplayName>
            <Value>0</Value>
         </EnumEntry>
         <pValue>transferControl_c</pValue>
      </Enumeration>
      <IntConverter Name="transferControl_c">
         <pVariable Name="MOD_REG">transferMode_Reg</pVariable>
         <FormulaTo>(MOD_REG&amp;1) + (FROM &amp;2)</FormulaTo>
         <FormulaFrom>(TO &amp;2)</FormulaFrom>
         <pValue>transferMode_Reg</pValue>
         <Slope>Varying</Slope>
      </IntConverter>
      <IntSwissKnife Name="transferContolAvailable">
         <pVariable Name="REGISTER_STREAMING">writeStreamingState</pVariable>
         <pVariable Name="XFERCONTROL">transferControl_c</pVariable>
         <Formula> (XFERCONTROL = 0) || REGISTER_STREAMING</Formula>
      </IntSwissKnife>
      <Enumeration Name="TransferOperationMode" NameSpace="Standard">
         <ToolTip>Sets the operation mode of the transfer</ToolTip>
         <Description>Sets the operation mode of the transfer.</Description>
         <DisplayName>Transfer Mode</DisplayName>
         <Visibility>Expert</Visibility>
         <pIsAvailable>transferContolAvailable</pIsAvailable>
         <Streamable>Yes</Streamable>
         <EnumEntry Name="Continuous">
            <ToolTip>Continuous transfer</ToolTip>
            <Description>Blocks are transferred continuously until stopped with the TransferStop command.</Description>
            <Value>0</Value>
         </EnumEntry>
         <EnumEntry Name="MultiBlock">
            <ToolTip>Transfer a Sequence of blocks</ToolTip>
            <Description>The transfer terminates after the transition on TransferBlockCount or before on a user request.</Description>
            <DisplayName>Multi Block</DisplayName>
            <Value>1</Value>
         </EnumEntry>
         <pValue>transferOperation_c</pValue>
      </Enumeration>
      <IntConverter Name="transferOperation_c">
         <pVariable Name="MOD_REG">transferMode_Reg</pVariable>
         <FormulaTo>(MOD_REG&amp;2) + (FROM &amp;1)</FormulaTo>
         <FormulaFrom>(TO &amp;1)</FormulaFrom>
         <pValue>transferMode_Reg</pValue>
         <Slope>Varying</Slope>
      </IntConverter>
      <Integer Name="transferRegCurentAddr">
         <Value>0x10000100</Value>
      </Integer>
      <IntReg Name="transferMode_Reg">
         <pAddress>transferRegCurentAddr</pAddress>
         <pAddress>persistenceOffset</pAddress>
         <Length>4</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
      <IntSwissKnife Name="transferBlockCountAvailable">
         <pVariable Name="REGISTER_STREAMING">writeStreamingState</pVariable>
         <pVariable Name="XFERMODE">transferMode_Reg</pVariable>
         <Formula>(XFERMODE = 1) || REGISTER_STREAMING</Formula>
      </IntSwissKnife>
      <Integer Name="TransferBlockCount" NameSpace="Standard">
         <ToolTip>Number of blocks to transfer</ToolTip>
