      <Boolean Name="ChunkEnable" NameSpace="Standard">
         <ToolTip>Metadata Enable</ToolTip>
         <Description>Sets the enable state of the selected metadata. When enabled, the metadata is included in the payload of the image.</Description>
         <DisplayName>Metadata Enable</DisplayName>
         <Visibility>Expert</Visibility>
         <Streamable>Yes</Streamable>
         <pValue>ChunkEnable_Converter</pValue>
      </Boolean>
      <IntConverter Name="ChunkEnable_Converter">
         <pIsLocked>TLParamsLocked</pIsLocked>
         <pVariable Name="CHUNK_ENABLE_RAW">ChunkEnable_Reg</pVariable>
         <pVariable Name="CHUNK_SELECTOR">chunkSelector_Value</pVariable>
         <FormulaTo>(FROM = 0)?(CHUNK_ENABLE_RAW &amp; (~CHUNK_SELECTOR)):(CHUNK_ENABLE_RAW | CHUNK_SELECTOR)</FormulaTo>
         <FormulaFrom>(TO &amp; CHUNK_SELECTOR &amp; 0xFFFFFFFF)?1:0</FormulaFrom>
         <pValue>ChunkEnable_Reg</pValue>
         <Slope>Varying</Slope>
      </IntConverter>
      <IntReg Name="ChunkEnable_Reg">
         <pIsAvailable>chunkActive</pIsAvailable>
         <Address>0x20004D30</Address>
         <Length>8</Length>
         <AccessMode>RW</AccessMode>
         <pPort>Device</pPort>
         <Sign>Unsigned</Sign>
         <Endianess>LittleEndian</Endianess>
      </IntReg>
